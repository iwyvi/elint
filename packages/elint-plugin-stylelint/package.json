{
  "name": "elint-plugin-stylelint",
  "version": "3.0.0",
  "description": "elint plugin for Stylelint",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --dts --format esm  --target node12.20"
  },
  "peerDependencies": {
    "stylelint": "^14.0.0"
  },
  "devDependencies": {
    "elint": "workspace:*",
    "stylelint": "^14.6.0"
  }
}
