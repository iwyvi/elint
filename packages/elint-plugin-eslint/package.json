{
  "name": "elint-plugin-eslint",
  "version": "3.0.0",
  "description": "elint plugin for ESLint",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --dts --format esm  --target node12.20"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "elint": "workspace:*",
    "eslint": "^8.11.0"
  }
}
